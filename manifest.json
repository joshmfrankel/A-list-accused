{
  "manifest_version": 2,
  "name": "Hollywood Offenders",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*.imdb.com/title/*"
      ],
      "css": [
        "css/styles.css"
      ],
      "js": [
        "js/jquery-3.2.1.min.js",
        "js/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
